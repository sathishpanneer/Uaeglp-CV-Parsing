@model Uaeglp.ViewModels.AssessmentViewModels.ReportConsolidateReportOverallView
@using  Uaeglp.ViewModels.Enums;
@{
    Layout = null;
    var ImageSrc = string.IsNullOrEmpty(Model.Profile.ImageSrc.ImgeIDEncrypted) ? "/Assets/Images/user-icon.png" : "/File/GetByEncryptedId?IdEncyrpted=" + Model.Profile.ImageSrc.ImgeIDEncrypted + "";
}

<html>
<head>
    <meta charset="utf-8">

    <style>
        @@font-face {
            font-family: DroidArabicKufi;
            src: url(../../Assets/fonts/DroidKufi-Regular.ttf);
        }

        .breakhere {
            page-break-after: always;
            background-color: blueviolet;
        }

        .imagetorotate {
            -webkit-transform: rotate(200deg);
            -webkit-backface-visibility: hidden;
        }

        body {
            font-family: DroidArabicKufi;
            direction: rtl;
            color: rgb(77,77,77);
        }

        .background {
            position: absolute;
            display: block;
            min-width: 100%;
            text-align: center;
            background-color: transparent;
            background: url(../../Assets/images/bgs/consolidated-report-ar-bg.jpg) no-repeat;
            background-size: contain;
            top: 0;
            left: 0;
            height: 100%;
            width: 100%;
            z-index: -1;
        }

        .text-red {
            color: rgb(150,24,35);
        }

        .text-orange {
            color: rgb(247,147,30);
        }

        .text-yellow {
            color: rgb(254,218,27);
        }

        .text-light-green {
            color: rgb(78,176,44);
        }

        .text-dark-green {
            color: rgb(5,133,67);
        }

        .text-white {
            color: #fff;
        }

        .bg-red {
            background-color: rgb(150,24,35);
        }

        .bg-orange {
            background-color: rgb(247,147,30);
        }

        .bg-yellow {
            background-color: rgb(254,218,27);
        }

        .bg-light-green {
            background-color: rgb(78,176,44);
        }

        .bg-dark-green {
            background-color: rgb(5,133,67);
        }

        .bg-white {
            background-color: #fff;
        }

        .font-size-12 {
            font-size: 12px;
        }

        .font-size-13 {
            font-size: 13px;
        }

        .font-size-14 {
            font-size: 14px;
        }

        .font-size-15 {
            font-size: 15px;
        }

        .font-size-16 {
            font-size: 16px;
        }

        .font-size-17 {
            font-size: 17px;
        }

        .font-size-22 {
            font-size: 22px;
        }

        .font-size-30 {
            font-size: 30px;
        }

        .font-weight-bold {
            font-weight: bold;
        }

        .sq-container .sq {
            width: 38px;
            height: 38px;
            float: right;
            margin-left: 5px;
        }

        .text-center {
            text-align: center;
        }

        .divider {
            width: 100%;
            height: 1px;
            background-color: #fff;
            overflow: hidden;
            margin-right: -15px;
        }

        .list-item {
            color: #666;
        }

        section .divider:last-child {
            display: none;
        }
